<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgress">

        <attr name="bg_path_color" format="color"/>
        <attr name="progress_color" format="color"/>
        <attr name="thumb_color" format="color"/>
        <attr name="strock_width" format="float"/>

        <attr name="emotion" format="enum">
            <enum name="sad" value="0"/>
            <enum name="norm" value="1"/>
            <enum name="happy" value="2"/>
        </attr>
        <attr name="src" format="reference"/>
    </declare-styleable>
</resources>